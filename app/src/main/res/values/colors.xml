    <color name="primaryColor">#1b2836</color>
    <!-- Primary dark color of the app (shown in the status bar) -->
    <color name="primaryDark">#141d26</color>
    <!-- color used for cardview background -->
    <color name="backgroundColorOverlay">#141d26</color>
    <!-- color used for cardview background when no thumbnail -->
    <color name="backgroundColorOverlayBlack">#000000</color>
